{
	"manifest_version": 2,
	"name": "Cheesto User Status",
	"version": "2.2.0",
	"author": "Onesimus Systems",
	"description": "Quickly access basic Dandelion functions.",
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	},
	"short_name": "Cheesto",
	"browser_action": {
		"default_title": "",
		"default_popup": "popup.html",
		"default_icon": "images/icon_default_action.png"
	},
	"icons": {
		"16": "images/icon_tiny_16.png",
		"48": "images/icon_small_48.png",
		"128": "images/icon_128.png"
	},
	"background": {
		"persistent": true,
		"scripts": [
			"scripts/jquery-3.2.1.min.js",
			"scripts/background.js"
		]
	},
	"permissions": [
		"http://*/",
		"https://*/",
		"storage",
		"webRequest",
		"contextMenus",
		"tabs"
	],
	"applications": {
		"gecko": {
			"id": "cheesto-chrome@onesimussystems.com"
		}
	}
}
